CONFIDENCE_THRESHOLD=0.7
USE_WEB_SEARCH=true
USE_MOCK_WEB_SEARCH=true

# TAVILY API 설정
TAVILY_API_KEY=tvly-xxxxxxxxxxxxxxxxxxxxxxxxxx

# Chroma DB 설정
COLLECTION_NAME=unified_rag_collection