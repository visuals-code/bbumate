# Upstage API 설정
UPSTAGE_API_KEY=YOUR_UPSTAGE_API_KEY

# Upstage 모델 설정
UPSTAGE_EMBEDDING_MODEL=solar-embedding-1-large
UPSTAGE_CHAT_MODEL=solar-1-mini-chat

# Vector DB 설정
